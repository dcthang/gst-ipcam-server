[pipeline]
pipeline-name = onesrc h264
pipeline-type = video
pipeline-codec = H264
pipeline-desc = ( v4l2src ! videoscale ! videorate ! video/x-raw-yuv,width=${video-width},height=${video-height},framerate=${video-framerate} ! ffmpegcolorspace ! ffenc_libx264 bitrate=${video-bitrate} ! rtph264pay name=pay0 pt=96 )

[onesrc h264-variables]
video-width=352
video-height=288
video-framerate=25/1
video-bitrate=2048
